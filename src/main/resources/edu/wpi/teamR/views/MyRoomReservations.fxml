<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamR.controllers.MyRoomReservationController">
    <top>
        <VBox alignment="CENTER" prefHeight="113.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
            <children>
                <AnchorPane prefHeight="111.0" prefWidth="1280.0" styleClass="room-title">
                    <children>
                        <HBox alignment="CENTER_RIGHT" layoutX="1080.0" layoutY="2.0" prefHeight="111.0" prefWidth="281.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <Button fx:id="newRequestBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="204.0" text="Book New Reservation">
                                    <HBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                  <HBox alignment="CENTER_LEFT" layoutY="6.0" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Button fx:id="deleteBtn" mnemonicParsing="false" text="Delete Selected">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                    </children></AnchorPane>
            </children></VBox>
    </top>
   <center>
      <StackPane fx:id="stackPane" alignment="TOP_CENTER" BorderPane.alignment="TOP_CENTER" />
   </center>
</BorderPane>
